{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "protostar4",
  "version": "4.0.0",
  "description": "Protostar4 is derived from the original Joomla Protostar template to work well in Joomla 4",
  "license": "GPL-2.0-or-later",
  "assets": [
    {
      "name": "template.protostar4.ltr",
      "type": "style",
      "uri": "template.min.css"
    },
    {
      "name": "template.protostar4.rtl",
      "type": "style",
      "uri": "template.min.css"
    },
    {
      "name": "template.offline",
      "type": "style",
      "uri": "offline.min.css"
    },
    {
      "name": "template.active.language",
      "type": "style",
      "uri": "",
      "class": "LangActiveAssetItem",
      "dependencies": [
        "template.active"
      ]
    },
    {
      "name": "template.user",
      "type": "style",
      "uri": "user.css",
      "dependencies": [
        "template.active",
        "template.active.language"
      ]
    },
    {
      "name": "template.protostar4",
      "type": "script",
      "uri": "template.js",
      "dependencies": [
        "core"
      ]
    },
    {
      "name": "template.active",
      "type": "script",
      "uri": "",
      "description": "A dummy asset to allow extensions to use it as a dependency to the active template",
      "dependencies": [
        "template.protostar4"
      ]
    },
    {
      "name": "template.user",
      "type": "script",
      "uri":  "user.js",
      "dependencies": [
        "template.active"
      ]
    },
    {
      "name": "template.protostar4.ltr",
      "type": "preset",
      "dependencies": [
        "template.protostar4.ltr#style",
        "template.protostar4#script"
      ]
    },
    {
      "name": "template.protostar4.rtl",
      "type": "preset",
      "dependencies": [
        "template.protostar4.rtl#style",
        "template.protostar4#script"
      ]
    }
  ]
}
